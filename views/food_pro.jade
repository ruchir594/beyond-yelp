    script(src='http://code.jquery.com/jquery.js')
    script.
        $(function () {
            $("#centerframe").on("click", function () {
                $("#baseframe").load("/network");
            });
            $("#leftframe").on("click", function () {
                $("#baseframe").load("/food");
            });
            $("#rightframe").on("click", function () {
                $("#baseframe").load("/me");
            });
        });
    each f,i in result
