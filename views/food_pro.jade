extends layout

block content
    title Your Profile
    nav#mainNav.navbar.navbar-default.navbar-fixed-top
        .container-fluid
            .navbar-header
                button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1')
                    span.sr-only Toggle navigation
                    span.icon-bar
                    span.icon-bar
                    span.icon-bar
                a.navbar-brand.page-scroll(itemprop='name', href='/') Networking over food
            #bs-example-navbar-collapse-1.collapse.navbar-collapse
                ul.nav.navbar-nav.navbar-right
                    li
                        a.page-scroll(href='/home') Home
                    li
                        a.page-scroll(href='/me/pro') My Profile
                    li
                        a.page-scroll(href='/network/pro') My Network
                    li
                        a.page-scroll(href='#services') Tech
                    li
                        a.page-scroll(href='/logout') Logout
    br
    br
    br
    #action.bg-primary
        .container
            .row
                .col-lg-8.col-lg-offset-2.text-center
                    h2.landing-heading.section-heading Welcome, #{user.firstname}
                    hr
                    h3.landing-heading
                        | Network with people, at your convenience, and theirs.
                #leftframe(style="float:left; width: 33%; height:120px; background-color:#EB3728;")
                    a(style="color: #FFFFEE;")
                        p.fa.fa-cutlery.fa-4x.centered Food
                #centerframe(style="float:left; width: 33%; height:120px; background-color:#EEEEEE;")
                    a(style="color: #444444")
                        p.fa.fa-handshake-o.fa-4x Network
                #rightframe(style="float:left; width: 33%; height:120px; background-color:#EEDB82;")
                    a
                        p.fa.fa-street-view.fa-4x Profile
    #baseframe(style="float:left, width: 100%; background-color:#DDDDDD;")
        #leftframeb(style="float:left; width: 100%; background-color:#EB3728;")
            br
            p
                | #{fillers.flr1} #{query.food} #{fillers.flr2} #{query.place}
            form.pure-form(name="findRestaurants", method="post", action="/find/pro")
                label(for="food") Get
                input.pure-input-1-3(type="text", name="food", placeholder="Thai Food; Burger; Ice Cream ... ")
                label(for="place") at
                input.pure-input-1-3(type="text", name="place", placeholder="Hanover, NH; London; Manhatten ...")
                button.pure-button.pure-button-primary(type="submit").fa.fa-search
            br
            br
            section#portfolio(style="background-color:#DDDDDD;")
              .container
                .row
                  .col-lg-12.text-center
                    p #{query.food} #{fillers.flr2} #{query.place}
                .row
                    each f,i in result
                        .col-sm-4.portfolio-item
                            a.portfolio-link(href='#sf'+i, data-toggle='modal', id='of'+i)
                                .caption
                                    .caption-content
                                        i.fa.fa-star(aria-hidden='true') #{f.rating}
                                        p #{f.price}
                                        i.fa.fa-search-plus.fa-3x
                                p(style="color:#444444; background-color:rgba(calc(125*2), 0, 10, calc(#{f.rating}*#{f.rating}/25)); ") #{f.name}
                                img.img-responsive(src='#{f.image_url}', alt='', style='max-height: 400px;')

